{
  "kind": "collectionType",
  "collectionName": "speakers",
  "info": {
    "singularName": "speaker",
    "pluralName": "speakers",
    "displayName": "Speaker",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "surname": {
      "type": "string",
      "required": true
    },
    "subline": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "speaker_headline": {
      "type": "string",
      "required": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "description": {
      "type": "richtext",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "draft",
        "review",
        "published",
        "archive"
      ],
      "default": "draft",
      "private": true,
      "required": true
    },
    "company": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
