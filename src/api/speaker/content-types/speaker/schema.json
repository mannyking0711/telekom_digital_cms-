    "twitter": {
      "type": "string"
    },
    "website": {
      "type": "string"
    },
    "facebook": {
      "type": "string"
    },
    "linkedin": {
      "type": "string"
    },
    "youtube": {
      "type": "string"
    },
    "xing": {
      "type": "string"
    },
    "instagram": {
      "type": "string"
    },
    "img_list": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "img_detail": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "img_header": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "fullname": {
      "type": "string",
      "required": true
    },
    "slug_de": {
      "type": "string"
