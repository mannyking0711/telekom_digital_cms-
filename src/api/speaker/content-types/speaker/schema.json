        "i18n": {
          "localized": true
        }
      }
    },
    "megatrends_text": {
      "type": "richtext",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "megatrends_subheadline": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
