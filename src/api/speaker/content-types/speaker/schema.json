      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "meta_description": {
      "type": "text",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "agenda_headline": {
      "type": "string",
      "required": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
